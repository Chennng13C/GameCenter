<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.final_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="android.example.final_project.BuyCar"></activity>
        <activity android:name="android.example.final_project.GiveActivity" />
        <activity android:name="android.example.final_project.BuyActivity" />
        <activity android:name="android.example.final_project.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="android.example.final_project.GamePage"
            android:label="@string/app_name"
            android:parentActivityName="android.example.final_project.MainActivity" />
    </application>

</manifest>